# PostgreSQL Database Configuration
POSTGRES_DB=multi_agent_panel
POSTGRES_USER=panel_user
POSTGRES_PASSWORD=changeme_secure_password
POSTGRES_PORT=5432

# Application Port
APP_PORT=8000

# Required: OpenAI API Key
OPENAI_API_KEY=sk-your-openai-key-here

# Optional: Additional LLM Provider API Keys
GEMINI_API_KEY=
CLAUDE_API_KEY=
ANTHROPIC_API_KEY=
GROK_API_KEY=
XAI_API_KEY=

# Optional: Tavily API Key for web search functionality
TAVILY_API_KEY=

# Optional: Use in-memory checkpointer instead of PostgreSQL (for testing)
# Set to 1 to disable PostgreSQL and use in-memory storage
USE_IN_MEMORY_CHECKPOINTER=0
